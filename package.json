{
  "name": "gvm-exportxls",
  "version": "1.0.0",
  "description": "Um aplicativo para se exportar os produtos do programa CIM para xls",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "app:dir": "electron-builder --dir",
    "app:dist": "electron-builder --win",
    "postinstall": "electron-builder install-app-deps"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DTineli/gvm-exportxls.git"
  },
  "build": {
    "appId": "your.id",
    "mac": {
      "category": "your.app.category.type"
    },
    "win": {
      "publisherName": "gvm"
    }
  },
  "keywords": [
    "excel",
    "GVM",
    "CIM",
    "Exportar"
  ],
  "author": "Daniel Tinelijr",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DTineli/gvm-exportxls/issues"
  },
  "homepage": "https://github.com/DTineli/gvm-exportxls#readme",
  "devDependencies": {
    "electron-builder": "^22.14.5",
    "electron": "^11.5.0"
  },
  "dependencies": {
    "csv": "^5.3.2",
    "ini": "^1.3.6",
    "mysql2": "^2.0.2",
    "xlsx": "^0.17.0"
  }
}
